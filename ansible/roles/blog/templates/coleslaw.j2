(:author "{{ blog_author }}"
 :deploy-dir "/srv/www/{{ blog_domain }}"
 :domain "http://{{ blog_domain }}"
 :feeds ("lisp")
 :plugins ((versioned)
           (mathjax)
           (static-pages)
           (disqus :shortname "{{ blog_disqus_shortname }}")
	   (analytics :tracking-code "{{ blog_analytics_code }}"))
 :routing ((:post "posts/~a")
           (:tag-index "tag/~a")
           (:month-index "date/~a")
           (:numeric-index "~d")
           (:feed "~a.xml")
           (:tag-feed "tag/~a.xml"))
 :sitenav ((:url "http://kingcons.io/" :name "Home")
           (:url "http://github.com/kingcons" :name "Code")
           (:url "http://soundcloud.com/redlinernotes" :name "Music")
           (:url "http://redlinernotes.com/docs/talks/" :name "Talks")
           (:url "http://twitter.com/king_cons" :name "Twitter"))
 :title "{{ blog_title }}"
 :theme "readable")
